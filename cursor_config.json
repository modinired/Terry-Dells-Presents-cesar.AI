{
  "cursor_agent": {
    "enabled": true,
    "agent_name": "Terry Delmonaco Cursor Agent",
    "version": "3.2",
    "api_endpoint": "https://api.cursor.ai/v1",
    "webhook_url": null,
    "auto_response": true,
    "real_time_sync": true,
    "context_window": 8192,
    "max_tokens": 4096,
    "capabilities": [
      "code_analysis",
      "file_operations",
      "terminal_commands",
      "web_search",
      "agent_communication",
      "task_delegation",
      "system_monitoring"
    ],
    "permissions": [
      "read_files",
      "write_files",
      "run_commands",
      "search_web",
      "communicate_with_agents",
      "access_system_info"
    ],
    "task_types": [
      "code_review",
      "bug_fix",
      "feature_development",
      "documentation",
      "testing",
      "deployment",
      "system_analysis"
    ],
    "priority_levels": [
      "critical",
      "high",
      "medium",
      "low"
    ],
    "auto_delegation": true,
    "collaboration_mode": true,
    "security": {
      "api_key_required": true,
      "webhook_verification": true,
      "rate_limiting": true,
      "max_requests_per_minute": 60
    },
    "integration": {
      "terry_delmonaco_system": true,
      "cesar_ecosystem": true,
      "background_agents": true,
      "external_platforms": true
    },
    "monitoring": {
      "performance_tracking": true,
      "error_reporting": true,
      "usage_analytics": true,
      "health_checks": true
    }
  },
  "environment_variables": {
    "CURSOR_AGENT_ENABLED": "true",
    "CURSOR_AGENT_NAME": "Terry Delmonaco Cursor Agent",
    "CURSOR_API_KEY": "your-cursor-api-key-here",
    "CURSOR_API_ENDPOINT": "https://api.cursor.ai/v1",
    "CURSOR_WEBHOOK_URL": "https://your-webhook-url.com/cursor",
    "CURSOR_PROJECT_ID": "your-project-id",
    "CURSOR_AUTO_RESPONSE": "true"
  },
  "deployment": {
    "docker_image": "terry-delmonaco/cursor-agent:3.2",
    "kubernetes_config": "cursor-agent-deployment.yaml",
    "vertex_ai_config": "cursor-vertex-ai.yaml",
    "environment": "production",
    "replicas": 2,
    "resources": {
      "cpu": "500m",
      "memory": "1Gi"
    }
  }
} 